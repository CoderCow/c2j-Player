  "scripts": {
    "postinstall": "typings install",
    "build:prod": "npm run clean && webpack --config webpack.prod.config.js --display-error-details && dotnet build webapi/c2j-web-api.csproj --configuration RELEASE",
    "build": "npm run clean && webpack --config webpack.dev.config.js --display-error-details && dotnet build webapi/c2j-web-api.csproj --configuration DEBUG",
    "build:api": "dotnet publish webapi/c2j-web-api.csproj --configuration DEBUG --output dist/netcoreapp1.1",
    "develop": "cd /webpack-dev-server --config webpack.dev.config.js",
    "develop:api": "cd webapi && dotnet watch publish c2j-web-api.csproj --configuration DEBUG --output dist/netcoreapp1.1",
    "test": "karma start --singleRun",
    "testing": "karma start",
    "coverage": "karma start karma.coverage.conf.js",
    "tslint": "tslint \"public/**/*.{ts,tsx}\"",
    "clean": "rimraf dist/*"
  },
